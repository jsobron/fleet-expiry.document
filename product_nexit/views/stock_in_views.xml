<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_stock_in_form" model="ir.ui.view">
            <field name="name">stock.in.form</field>
            <field name="model">stock.in</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="quantity"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <record id="view_stock_in_tree" model="ir.ui.view">
            <field name="name">stock.in.tree</field>
            <field name="model">stock.in</field>
            <field name="arch" type="xml">
                <tree>
                   
                    <field name="quantity"/>
                    <field name="move_date"/>
                </tree>
            </field>
        </record>

        <record id="view_stock_in_search" model="ir.ui.view">
            <field name="name">stock.in.search</field>
            <field name="model">stock.in</field>
            <field name="arch" type="xml">
                <search>
                   
                    <field name="quantity"/>
                </search>
            </field>
        </record>

        <record id="action_stock_in_form" model="ir.actions.act_window">
            <field name="name">Stock In</field>
            <field name="res_model">stock.in</field>
            <field name="view_mode">form</field>
            <field name="type">ir.actions.act_window</field>
        </record>

        <menuitem id="menu_stock_in_list" name="Stock in lista" parent="menu_product" action="action_stock_in_form" sequence="3"/>
    </data>
</odoo>