<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_stock_out_form" model="ir.ui.view">
            <field name="name">stock.out.form</field>
            <field name="model">stock.out</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="quantity"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <record id="view_stock_out_tree" model="ir.ui.view">
            <field name="name">stock.out.tree</field>
            <field name="model">stock.out</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="quantity"/>
                </tree>
            </field>
        </record>

        <record id="view_stock_out_search" model="ir.ui.view">
            <field name="name">stock.out.search</field>
            <field name="model">stock.out</field>
            <field name="arch" type="xml">
                <search>
                    <field name="quantity"/>
                </search>
            </field>
        </record>

        <record id="action_stock_out_form" model="ir.actions.act_window">
            <field name="name">Stock Out</field>
            <field name="res_model">stock.out</field>
            <field name="view_mode">form</field>
            <field name="type">ir.actions.act_window</field>
        </record>

        <menuitem id="menu_stock_out_list" name="Stock out lista" parent="menu_product" action="action_stock_in_form" sequence="3"/>
    </data>
    
</odoo>